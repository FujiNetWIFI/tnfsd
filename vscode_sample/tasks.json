{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "GNU Make Project",
            "command": "mingw32-make.exe",
            "args": [
                "all"
            ],
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "DEBUG":"Y"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}